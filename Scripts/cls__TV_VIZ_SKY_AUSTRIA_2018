            SetVizDataUpper ids+1,Data.Item("TABNAME"&i)
            SetVizData ids+2,Data.Item("TABGAMES"&i)
            SetVizData ids+3,Data.Item("TABGOALS"&i)
            SetVizData ids+4,Data.Item("TABDIFF"&i)
            SetVizData ids+5,Data.Item("TABPTS"&i)

            SetVizDataBufferOff
            ids=ids+100
        next
        
        SetVizDataUpper "4000", Data.Item("REHL")
        ids=4005

        for i=1 to 6
           SetVizDataBufferOn
            SetVizData ids,0
            SetVizDataUpper ids+5,Data.Item("RENAMEA" & i)
            SetTeamLogo ids+10, Data.Item("RELOGOA" & i)
            SetVizDataUpper ids+15,Data.Item("RENAMEB" & i)
            SetTeamLogo ids+20, Data.Item("RELOGOB" & i)
            SetVizData ids+1,Data.Item("RERESULT" & i)

            SetVizDataBufferOff
            ids=ids+1000
        next

        GetPreviewWF "Default 2.82", dir&" 2.5", sScene
        Redim ActionList(2, 1)
        ActionList(1, 0) = "Default"
        ActionList(1, 1) = "Default"
        ActionList(2, 1) = efPlay
    end sub


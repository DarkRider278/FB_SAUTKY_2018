    public sub BB_Score_WF(ByRef Data)
        dim i
        ' Wieviele TS
        dim c
        c= Data.Item("ITEMCOUNT")
        sScene = "BB_MatchID"
        if c>0 then
           sScene = "BB_Torschuetze"
        end if

        LoadSceneWF elMainLayer, sScene
        SetCommercialOnOffManual true, "BB Score"
        SetVizData "0102",UCase(Data.Item("SNAME1")   )
        SetVizData "0104",UCase(Data.Item("SNAME2") )

        'SetTName  Data.Item("NAME1")
        'SetTName2 Data.Item("NAME2")


        SetTLogo LCase(Data.Item("LOGO1"))
        SetTLogo2_WF LCase(Data.Item("LOGO2"))

        SetVizData "0105",UCase(GetString("ABSCHNITT_" & Data.Item("ROUND"), i__TVLanguage) )
        SetVizData "0103", Data.Item("SCORE")


        dim ids
        ids=110

        for i = 1 to (Data.Item("ITEMCOUNT"))
            if Data.Item("NAME_A"& i)<>"" then
                SetVizData "0"&(ids) ,UCase( Data.Item("NAME_A"& i) &" ("&Data.Item("MIN_A"& i)&")" )
            else
                SetVizData "0"&(ids),""
            end if
            if Data.Item("NAME_B"& i)<>"" then
                SetVizData "0"&(ids+100) ,UCase( Data.Item("NAME_B"& i) &" ("&Data.Item("MIN_B"& i)&")" )
            else
                SetVizData "0"&(ids+100),""
            end if
            ids=ids+5
        next
        for i = (1+Data.Item("ITEMCOUNT")) to 10
            SetVizData "0"&(ids) , ""
            SetVizData "0"&(ids+100) , ""
            ids=ids+5
        next
        
        if c<3 then
            SetVizData "0002",0
        elseif c<5 then
            SetVizData "0002",1
        elseif c<7 then
            SetVizData "0002",2
        elseif c<9 then
            SetVizData "0002",3
        else
            SetVizData "0002",4
        end if
        GetPreviewWF "Default 1.86", "TORE 1.0", sScene
        Redim ActionList(2, 1)
        ActionList(1, 0) = "Default"
        ActionList(1, 1) = "Default"
        ActionList(2, 1) = efPlay
    end sub


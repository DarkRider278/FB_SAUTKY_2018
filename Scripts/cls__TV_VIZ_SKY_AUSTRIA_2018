        if(pos>0 )then
            fn=Trim(Left(p,pos))
            n=Right(p,Len(p)-pos)
        else
            n=p
            fn=""
        end if
        SetVizData 1101,info&n
        dim min2
        min2=min
        if min <> "" then
            min2 = "(" &min & ")"
        end if
        SetVizData 1102, min2

        GetPreview "Default", "2.0", sScene
        Redim ActionList(2, 1)
        ActionList(1, 0) = "Default"
        ActionList(1, 1) = "Default"
        ActionList(2, 1) = efPlay
    end sub
    

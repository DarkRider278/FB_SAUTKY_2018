                else
                    obj__TV.SendCommand "RENDERER*FRONT_LAYER*TREE*$BLITZTABELLE$LINE2$img_cl_icon*ACTIVE SET 0"
                    obj__TV.SendCommand "RENDERER*FRONT_LAYER*TREE*$BLITZTABELLE$LINE2$gfx_area_Line1*MATERIAL*COLOR SET 1.0 1.0 1.0"
                end if
            end if
            if k=3 then

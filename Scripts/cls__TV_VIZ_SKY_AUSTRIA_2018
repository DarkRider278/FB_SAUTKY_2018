                    obj__TV.SendCommand "RENDERER*FRONT_LAYER*TREE*$BLITZTABELLE$LINE3$gfx_area_Line1*MATERIAL*COLOR SET 0.902 0.902 0.902"
                else
                    obj__TV.SendCommand "RENDERER*FRONT_LAYER*TREE*$BLITZTABELLE$LINE3$img_cl_icon*ACTIVE SET 0"
                    obj__TV.SendCommand "RENDERER*FRONT_LAYER*TREE*$BLITZTABELLE$LINE3$gfx_area_Line1*MATERIAL*COLOR SET 1.0 1.0 1.0"
